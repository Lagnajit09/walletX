{
  "buildCommand": "cd ../.. && npm install && npx turbo run build --filter=bank-webhook...",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "dist/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/hdfcWebhook",
      "dest": "dist/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}