{
  "buildCommand": "cd ../.. && npm install && cd packages/db && npx prisma generate && cd ../../apps/bank-webhook && npx turbo run build --filter=bank-webhook...",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "../../packages/db/node_modules/.prisma/**",
          "../../packages/db/prisma/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "dist/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/hdfcWebhook",
      "dest": "dist/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}