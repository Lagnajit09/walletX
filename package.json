{
  "name": "bank-webhook",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prebuild": "prisma generate",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --external:@prisma/client --external:express",
    "start": "node dist/index.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "esbuild": "^0.20.2",
    "prisma": "^5.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
